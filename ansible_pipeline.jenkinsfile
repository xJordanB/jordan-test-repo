pipeline {
    agent any
    parameters {
        string(name: 'PACKAGE', defaultValue: 'None', description: 'Please specify the package which you wish to deploy.')
    }
    stages {
        stage('Deploy New Subnet') {
            steps {
                sh "sudo -u ansible_adm -s"
                sh "whoami"
                sh "ansible all -s -m yum -a "name=httpd state=present"
            }
        }
    }
}